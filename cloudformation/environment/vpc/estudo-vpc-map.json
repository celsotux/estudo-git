{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Criação de um namespace do AWS CloudMap para estudo",
    "Resources": {
      "CloudMapNamespace": {
        "Type": "AWS::ServiceDiscovery::PrivateDnsNamespace",
        "Properties": {
          "Name": "estudo-namespace.internal",
          "Vpc": { "Ref": "EstudoVPC" }
        }
      }
    }
  }
  