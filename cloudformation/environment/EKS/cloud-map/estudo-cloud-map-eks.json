{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Cria um namespace do AWS Cloud Map incorporando a VPC estudo-vpc-eks",
    "Parameters": {
      "VpcStackName": {
        "Type": "String",
        "Description": "Nome da stack da VPC (estudo-vpc-eks)"
      }
    },
    "Resources": {
      "CloudMapNamespace": {
        "Type": "AWS::ServiceDiscovery::PrivateDnsNamespace",
        "Properties": {
          "Name": "myapp.local",
          "Vpc": {
            "Fn::ImportValue": {
              "Fn::Sub": "${VpcStackName}-VPCID"
            }
          }
        }
      }
    }
  }
  